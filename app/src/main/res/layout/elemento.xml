<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/elementoNombre"
        android:layout_width="191dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:text="ID-delObjeto"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/elementoImagen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="383dp"
        android:layout_height="87dp"
        android:layout_marginBottom="11dp"
        android:layout_marginEnd="1dp"
        android:layout_marginTop="1dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="116dp"
            android:layout_height="64dp"
            android:layout_marginBottom="28dp"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/webapp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="198dp"
            android:layout_height="64dp"
            android:layout_marginBottom="28dp"
            android:layout_marginEnd="36dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo" />

    </android.support.constraint.ConstraintLayout>

    <ImageView
        android:id="@+id/elementoImagen"
        android:layout_width="287dp"
        android:layout_height="135dp"
        android:layout_marginBottom="13dp"
        android:layout_marginEnd="54dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="20dp"
        android:adjustViewBounds="false"
        android:background="@android:drawable/screen_background_dark_transparent"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/elementoTexto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elementoNombre"
        app:srcCompat="@color/cast_expanded_controller_ad_container_white_stripe_color" />

    <TextView
        android:id="@+id/elementoTexto"
        android:layout_width="340dp"
        android:layout_height="149dp"
        android:layout_marginBottom="9dp"
        android:layout_marginEnd="26dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="13dp"
        android:text="Contenido"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/Bescaner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elementoImagen" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="98dp"
        android:layout_height="34dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="11dp"
        android:text="Nombre"
        android:textAlignment="textEnd"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/elementoImagen"
        app:layout_constraintEnd_toStartOf="@+id/elementoNombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

    <Button
        android:id="@+id/Bescaner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:layout_marginEnd="28dp"
        android:layout_marginTop="9dp"
        android:background="@color/cast_expanded_controller_ad_label_background_color"
        android:text="Scaner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Binicio"
        app:layout_constraintTop_toBottomOf="@+id/elementoTexto" />

    <Button
        android:id="@+id/Binicio"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:layout_marginEnd="164dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="9dp"
        android:background="@color/cast_expanded_controller_ad_label_background_color"
        android:elevation="@dimen/cast_expanded_controller_seekbar_disabled_alpha"
        android:text="Inicio"
        android:textColor="@color/cast_expanded_controller_background_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Bescaner"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elementoTexto"
        app:layout_constraintVertical_bias="0.6" />

</android.support.constraint.ConstraintLayout>